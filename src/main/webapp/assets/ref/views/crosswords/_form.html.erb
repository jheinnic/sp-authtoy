      <td><span class="label label-success">$300</span></td><td><span data-prizeid="13" class="badge badge-info">0</span></td>
    </tr>
    <tr>
      <td><span class="label label-info">$12</span></td><td><span data-prizeid="5" class="badge badge-info">0</span></td>
      <td><span class="label label-success">$1,000</span></td><td><span data-prizeid="14" class="badge badge-info">0</span></td>
    </tr>
