    </tr>
    <tr>
      <td colspan="6"><img alt="card19" src="<%= path_to_image 'card/card_19.jpg' %>"></td>
    </tr>
  </table>
</div>
<div class="span3">
  <% if @crossword.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@crossword.errors.count, "error") %> prohibited this crossword from being saved:</h2>
    <ul><% @crossword.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %></ul>
  </div>
  <% end %>
  <div id="debug_messages"></div>
  <%= form_for(@crossword) do |f| %>
    <%= f.hidden_field :bonus_value %><%= f.hidden_field :bonus_word %>
    <%= f.fields_for :word_items do |w| %>
      <%= w.hidden_field :x_coordinate %><%= w.hidden_field :y_coordinate %><%= w.hidden_field :is_horizontal %><%= w.hidden_field :text_value %><%= w.hidden_field :triple_letter_index %>
    <% end %>
  <% end %>
  <table id="wordsummary" class="table table-condensed"><% for ii in (0..10) %>
    <tr><td><span class="badge badge-info"></span></td><td><span class="badge badge-info"></span></td></tr>
  <% end %></table>
